<p id="notice"><%= notice %></p>

<h1>Listing Postreplies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postreplies.each do |postreply| %>
      <tr>
        <td><%= link_to 'Show', postreply %></td>
        <td><%= link_to 'Edit', edit_postreply_path(postreply) %></td>
        <td><%= link_to 'Destroy', postreply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Postreply', new_postreply_path %>
